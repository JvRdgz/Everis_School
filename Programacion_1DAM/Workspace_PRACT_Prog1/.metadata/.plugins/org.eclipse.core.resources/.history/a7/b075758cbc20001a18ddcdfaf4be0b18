package main;

public class Prueba {

	public static void main(String[] args) {
		System.out.println("             -   /-----------/");
		System.out.println("            /   /|          /|");
		System.out.println("           /   / |         / |");
		System.out.println("          /   /  /--------/--");
		System.out.println("        l/   /  /        /  /");
		System.out.println("        /   /  /        /  /");
		System.out.println("       /   /  /        /  /");
		System.out.println("      /   /  /        /  /");
		System.out.println("     /   /  /        /  /");
		System.out.println("    -   /  /        /  /");
		System.out.println("   -   -----------   / ");
		System.out.println("   |  | /         | /");
		System.out.println("  h|  |/          |/");
		System.out.println("   -   -----------");

		System.out.println("      <--------->  ");
		System.out.println("           w       ");
	}
}
