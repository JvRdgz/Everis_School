package main;

public class Prueba {

	public static void main(String[] args) {
		System.out.println("             /-----------/");
		System.out.println("            /|          /|");
		System.out.println("           / |         / |");
		System.out.println("          /  /--------/--");
		System.out.println("         /  /        /  /");
		System.out.println("        /  /        /  /");
		System.out.println("       /  /        /  /");
		System.out.println("      /  /        /  /");
		System.out.println("     /  /        /  /");
		System.out.println("    /  /        /  /");
		System.out.println("    -----------   /");
		System.out.println("   | /         | / ");
		System.out.println("   |/          |/ ");
		System.out.println("    -----------");

		System.out.println("         w       ");
		System.out.println("    <--------->  ");
	}
}
